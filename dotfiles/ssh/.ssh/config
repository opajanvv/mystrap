# GitHub
Host github.com
  IdentityFile ~/.ssh/github
  IdentitiesOnly yes

# GitLab
Host gitlab.com
  IdentityFile ~/.ssh/gitlab
  IdentitiesOnly yes

# Proxmox server
Host server server.local
  Hostname 192.168.144.10
  User jan

# Default: use machine-specific key (%L = local hostname)
Host *
  User jan
  IdentityFile ~/.ssh/%L
  IdentitiesOnly yes
